<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.myclick.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_dogFragment2"
            app:destination="@id/dogFragment2" />
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.myclick.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.myclick.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/cardFragment"
        android:name="com.example.myclick.ui.cards.CardFragment"
        android:label="My To Do App"
        tools:layout="@layout/fragment_card" />
    <dialog
        android:id="@+id/dogFragment2"
        android:name="com.example.myclick.ui.dialog.DogFragment"
        android:label="fragment_dog"
        tools:layout="@layout/fragment_dog" />
</navigation>